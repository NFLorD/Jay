<?php

require_once 'config.php';

$directory = new RecursiveDirectoryIterator(CACHE_DIR);
$iterator = new RecursiveIteratorIterator($directory);

foreach ($iterator as $info) {
    if ($info->isFile()) {
        $filename = $info->getFilename();
        if ($filename[0] !== '.') {
            unlink(CACHE_DIR . '/' . $filename);
        }
    }
}

die;